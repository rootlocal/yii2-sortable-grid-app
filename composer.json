{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "~2.0.2",
    "yiisoft/yii2-symfonymailer": "~2.0.3",
    "rootlocal/yii2-sortable-grid-view": "*",
    "cebe/markdown": "^1.0",
    "cebe/markdown-latex": "^1.0",
    "yiisoft/yii2-apidoc": "dev-master",
    "yiisoft/yii2-faker": "~2.0.0",
    "kartik-v/yii2-widget-datepicker": "^1.4",
    "rmrevin/yii2-fontawesome": "~2.17",
    "a1inani/yii2-modal-ajax": "@dev"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "phpunit/phpunit": "~9.5.0",
    "codeception/codeception": "^5.0.0 || ^4.0",
    "codeception/lib-innerbrowser": "^3.0 || ^1.1",
    "codeception/module-asserts": "^3.0 || ^1.1",
    "codeception/module-yii2": "^1.1",
    "codeception/module-filesystem": "^3.0 || ^1.1",
    "codeception/verify": "^2.2",
    "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4",
    "symfony/var-dumper": "^3.4",
    "squizlabs/php_codesniffer": "3.*",
    "phploc/phploc": "^7.0",
    "pdepend/pdepend": "^2.10",
    "phpmd/phpmd": "^2.12",
    "sebastian/phpcpd": "^6.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "rootlocal/yii2-sortable-grid-view"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
